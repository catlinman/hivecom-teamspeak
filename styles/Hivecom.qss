}

QTreeView::branch:hover, QTreeView::branch:selected:hover {
    background: #101010;
    color: #c9ff7d;
}

QTreeView::branch:selected {
    background: #101010;
}

QTreeView::item:selected:active, QTreeView::item:selected:!active {
    background: #101010;
    color: #a8fd30;
    outline: none;
}

QTreeView::item:hover, QTreeView::item:selected:hover {
    background: #101010;
    color: #c9ff7d;
}

QTreeView#ServerTreeView::item:selected:active, QTreeView#ServerTreeView::item:selected:!active {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0.05 rgba(168, 253, 48, 0), stop: 0.45 rgba(168, 253, 48, 0.05), 
        stop: 0.55 rgba(168, 253, 48, 0.05), stop: 0.95 rgba(168, 253, 48, 0)
    );

    border-bottom: 1px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0.0 rgba(168, 253, 48, 0.02), stop: 0.4 rgba(168, 253, 48, 0.2), 
        stop: 0.6 rgba(168, 253, 48, 0.2), stop: 1.0 rgba(168, 253, 48, 0.02)
    );
    
    border-top: 1px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0.0 rgba(168, 253, 48, 0.02), stop: 0.4 rgba(168, 253, 48, 0.2), 
        stop: 0.6 rgba(168, 253, 48, 0.2), stop: 1.0 rgba(168, 253, 48, 0.02)
    );

    color: #c9ff7d;
    outline: none;
}

QTreeView#ServerTreeView::item:hover, QTreeView#ServerTreeView::item:selected:hover {
    background: qlineargradient(
        x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0.05 rgba(168, 253, 48, 0), stop: 0.45 rgba(168, 253, 48, 0.05), 
        stop: 0.55 rgba(168, 253, 48, 0.05), stop: 0.95 rgba(168, 253, 48, 0)
    );

    border-bottom: 1px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
        stop: 0.0 rgba(168, 253, 48, 0.02), stop: 0.4 rgba(168, 253, 48, 0.2), 
        stop: 0.6 rgba(168, 253, 48, 0.2), stop: 1.0 rgba(168, 253, 48, 0.02)
    );
    
    border-top: 1px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
